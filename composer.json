{
  "name": "tourze/yii2-base",
  "description": "Split base directory from yii2 framework",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "maglnet/composer-require-checker": "^4",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "yii\\base\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "yii\\base\\tests\\": "tests/"
    }
  }
}